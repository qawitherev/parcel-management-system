{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateLimitSettings": {
    "Global": {
      "PermitLimit": 1000,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 4,
      "QueueLimit": 0
    },
    "User": {
      "PermitLimit": 1000,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 2,
      "QueueLimit": 0
    },
    "StrictEndpoint": {
      "PermitLimit": 100,
      "WindowMinutes": 10,
      "SegmentsPerWindow": 1,
      "QueueLimit": 0
    }
  },
  "AllowedHosts": "*"
}
